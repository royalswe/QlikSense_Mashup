{
    "_themeName": "Qlik Sense Default",
    "_variables": {
        "B20": "#333333",
        "B35": "#595959",
	    "B50": "#808080",
        "B60": "#999999",
        "B80": "#cccccc",
        "B90": "#E6E6E6",
        "B100": "#ffffff",
        "titleColor": "@B20",
        "disabledColor": "@B60",
        "lockedColor": "#6abbdd",
        "labelHighContrast": "@B35",
        "labelLowContrast": "@B90",
        "lineMajor": "@B80",
        "lineMinor": "@B90",
        "lineHighContrast": "@B60"
    },
    "backgroundColor": "#fff",
    "fontFamily": "QlikView Sans",
    "fontSize": "13px",
    "fontWeight": "normal",
    "color": "@labelHighContrast",
    "axis": {
        "title": {
            "fontSize": {
                "default": "12px",
                "xsmall": "12px",
                "small": "12px",
                "medium": "12px",
                "full": "14px"
            },
            "color": {
                "default": "@titleColor",
                "active": "@titleColor",
                "inactive": "@disabledColor",
                "locked": "@lockedColor",
                "disabled": "@disabledColor"
            }
        },
        "label": {
            "fontSize": {
                "default": "12px",
                "xsmall": "10px",
                "small": "11px",
                "medium": "12px",
                "full": "14px"
            },
            "discrete": {
                "fontSize": {
                    "default": "12px",
                    "xsmall": "10px",
                    "small": "11px",
                    "medium": "12px",
                    "full": "12px"
                }
            },
            "margin": 4
        },
        "tick": {
            "major": {
                "color": "@lineMajor",
                "length": 8
            },
            "minor": {
                "color": "@lineMinor",
                "length": 8
            }
        },
        "line": {
            "color": "@lineMajor"
        },
        "grid": {
            "major": {
                "color": "@lineMajor"
            },
            "minor": {
                "color": "@lineMinor"
            },
            "zero": {
                "color": "@lineHighContrast"
            },
            "width": {
                "default": 1,
                "full": 2,
                "medium": 1,
                "small": 1,
                "xsmall": 1,
                "spark": 1
            }
        }
    },
    "referenceLine": {
        "label": {
            "color": "@titleColor",
            "fontSize": "12px"
        },
        "outOfBounds": {
            "color": "@labelLowContrast",
            "backgroundColor": "#737373",
            "fontSize": "10px"
        }
    },
    "legend": {
        "title": {
            "color": "@titleColor",
            "fontSize": "15px",
            "lineHeight": "1em"
        },
        "label": {
            "color": "@labelHighContrast",
            "fontSize": "13px",
            "lineHeight": "1em"
        }
    },
    "item": {
        "name": {
            "color": "@labelHighContrast",
            "fontSize": "13px"
        },
        "value": {
            "color": "@labelHighContrast",
            "fontSize": "12px"
        }
    },
    "gauge": {
        "value": {
            "color": "@titleColor",
            "fontSize": "42px",
            "minFontSize": 12,
            "maxFontSize": 60
        },
        "area": {
            "backgroundColor": "#eeeeee"
        },
        "pointer": {
            "color": {
                "light": "@B90",
                "dark": "@B20"
            }
        }
    },
    "treemap": {
        "branch": {
            "fill": "rgba(0,0,0,0.05)",
            "label": {
                "color": "rgba(50,50,50,1)",
                "fontSize": "14px"
            }
        },
        "leaf": {
            "border": {
                "width": {
                    "default": 1,
                    "selected": 2
                },
                "color": {
                    "default": "#eee",
                    "selected": "#000000"
                }
            },
            "label": {
                "fontSize": "12px",
                "color": {
                    "default": "@labelHighContrast",
                    "light": "@labelLowContrast",
                    "dark": "@labelHighContrast"
                }
            }
        },
        "overlay": {
            "label": {
                "color": {
                    "default": "rgba(33,33,33,0.4)",
                    "light": "rgba(222,222,222,0.4)",
                    "dark": "rgba(33,33,33,0.4)"
                },
                "fontWeight": "bold"
            }
        }
    },
    "scatterPlot": {
        "value": {
            "color": "@labelHighContrast",
            "fontSize": "12px",
            "background": "rgba(255,255,255,0.9)"
        },
        "shape": {
            "border": {
                "width": {
                    "default": 1,
                    "selected": 2
                },
                "color": "rgba(0,0,0,0.7)"
            },
            "outOfBounds": {
                "opacity": 0.3
            }
        },
        "line": {
            "color": "@lineHighContrast",
            "width": 1
        }
    },
    "barChart": {
        "shape": {
            "border": {
                "color": {
                    "default": "rgba(0,0,0,0.25)",
                    "selected": "@titleColor"
                },
                "width": {
                    "default": 0,
                    "selected": 2
                }
            }
        },
        "value": {
            "color": {
                "default": "@labelHighContrast",
                "dark": "@labelHighContrast",
                "light": "@labelLowContrast"
            },
            "fontSize": {
                "default": "11px",
                "full": "11px",
                "medium": "11px",
                "small": "11px",
                "xsmall": "11px",
                "spark": "11px"
            }
        }
    },
    "lineChart": {
        "fontSize": "13px",
        "color": "@labelHighContrast",
        "item": {
            "value": {
                "color": {
                    "default": "@labelHighContrast",
                    "dark": "@labelHighContrast",
                    "light": "@labelLowContrast"
                },
                "fontSize": {
                    "default": "12px",
                    "full": "12px",
                    "medium": "12px",
                    "small": "12px",
                    "xsmall": "12px",
                    "spark": "12px"
                }
            },
            "shape": {
                "size": {
                    "default": 6,
                    "selected": 10
                }
            },
            "line": {
                "width": {
                    "default": 1,
                    "full": 2,
                    "medium": 1,
                    "small": 1,
                    "xsmall": 1,
                    "spark": 1
                }
            },
            "outOfRange": {
                "color": "@lineHighContrast"
            }
        }
    },
    "pieChart": {
        "fontSize": "13px",
        "color": "@labelHighContrast",
        "axis": {
            "title": {
                "fontSize": "14px",
                "color": {
                    "default": "@titleColor",
                    "active": "@titleColor",
                    "inactive": "@disabledColor"
                }
            }
        },
        "item": {
            "name": {
                "fontSize": {
                    "default": "13px",
                    "large": "16px",
                    "medium": "14px",
                    "small": "12px",
                    "tiny": "10px"
                },
                "color": {
                    "default": "@labelHighContrast",
                    "dark": "@titleColor",
                    "light": "@labelLowContrast"
                }
            },
            "value": {
                "color": {
                    "default": "@labelHighContrast",
                    "dark": "@titleColor",
                    "light": "@labelLowContrast"
                },
                "fontSize": {
                    "default": "12px",
                    "large": "14px",
                    "medium": "12px",
                    "small": "10px",
                    "tiny": "9px"
                }
            },
            "shape": {
                "border": {
                    "width": {
                        "default": 1,
                        "selected": 2
                    },
                    "color": {
                        "default": "#eeeeee",
                        "selected": "@titleColor"
                    }
                }
            },
            "line": {
                "width": 1,
                "color": "@lineHighContrast"
            }
        }
    },
    "boxPlot": {
        "box": {
            "whisker": {
                "stroke": "@B35"
            },
            "line": {
                "stroke": "@B35"
            },
            "box": {
                "fill": "@B90",
                "stroke": "@B35"
            }
        }
    },
    "distributionPlot": {
        "box": {
            "fill": "@B90",
            "stroke": "@B80"
        }
    },
	"waterfallChart": {
	  "value": {
		"color": {
		  "default": "@labelHighContrast",
		  "dark": "@labelHighContrast",
		  "light": "@labelLowContrast"
		}
	  },
	  "shape": {
		"positiveValue": {
		  "fill": "#4477AA"
		},
		"negativeValue": {
		  "fill": "#cc6677"
		},
		"subtotal": {
		  "fill": "#c3c3c3"
		},
		"bridge": {
		  "stroke": "@B50"
		}
	  }
	},
	"mapChart": {
		"label": {
			"value": {
				"color": "@B35",
				"fontSize": "12px",
				"fontFamily": "QlikView Sans",
				"backgroundColor": "transparent"
			}
		}
	},
